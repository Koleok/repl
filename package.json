{
  "name": "ramda-repl",
  "description": "Ramda + Ramda Fantasy + Sanctuary REPL",
  "version": "1.0.0",
  "dependencies": {
    "babel-core": "6.16.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babelify": "7.3.0",
    "babylon": "6.11.2",
    "browserify": "^13.1.0",
    "cliquer": "^0.1.4",
    "date-fns": "^1.28.0",
    "debounce": "1.0.0",
    "fluture": "^5.0.0",
    "immutable": "^3.8.1",
    "immutable-ext": "^1.0.8",
    "jsverify": "^0.7.4",
    "monoid": "^0.2.0",
    "pretty-js": "^0.1.8",
    "ramda": "^0.23.0",
    "ramda-fantasy": "0.7.0",
    "ramda-lens": "^0.1.1",
    "rdom": "^0.6.0",
    "sanctuary": "^0.12.2",
    "sanctuary-def": "^0.9.0",
    "seamless-immutable": "^7.0.1",
    "through2": "2.0.1",
    "vinyl-fs": "2.4.3",
    "z": "z-pattern-matching/z"
  },
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/ramda/ramda.github.io.git"
  },
  "scripts": {
    "test": "mocha",
    "server": "./node_modules/.bin/http-server -p 9999 -o",
    "lint": "./node_modules/.bin/eslint lib/js/*.js && eslint test/*.js",
    "dev": "./node_modules/.bin/watchify ./lib/js/main.js -t babelify ./lib/js/repl.js -o ./dist/bundle.js -v",
    "build": "./node_modules/.bin/browserify ./lib/js/main.js -t babelify ./lib/js/repl.js -o ./dist/bundle.js",
    "style": "./node_modules/.bin/postcss -u postcss-import -u cssnano -u autoprefixer 'last 2 versions' -o ./dist/bundle.css ./lib/css/repl.css",
    "clean": "rm -rf ./dist && mkdir dist",
    "bundle": "npm run clean && npm run build && npm run style",
    "image": "npm run bundle && docker build -t koleok/repl ."
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "codemirror": "^5.19.0",
    "cssnano": "^3.7.5",
    "eslint": "^3.7.0",
    "fake-xml-http-request": "^1.4.0",
    "http-server": "^0.9.0",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.1.0",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2",
    "sinon": "^1.17.6",
    "watchify": "^3.7.0"
  }
}
